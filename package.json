{
  "name": "dotoo-ut",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://space-angel.github.io/dotooTest/",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
